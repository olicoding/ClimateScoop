{
  "name": "dciconn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"node api-server\"",
    "dev:start": "next dev",
    "start": "next start",
    "start:both": "concurrently \"next start\" \"node api-server\"",
    "start:api": "node api-server",
    "clean": "rm -rf .next",
    "build": "next build",
    "pipeline": "npm run lint && npm run test && npm run cypress:run",
    "lint": "eslint . --ext .js,.jsx",
    "test": "jest",
    "test:openHandles": "jest --detectOpenHandles",
    "test:watch": "jest --coverage --watch",
    "test:integration": "start-server-and-test start http-get://localhost:3000 cypress:run",
    "test:integration:watch": "start-server-and-test start http-get://localhost:3000 cypress:open",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.0.0",
    "@nivo/core": "^0.83.1",
    "@nivo/line": "^0.83.1",
    "axios": "^1.6.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.0",
    "helmet": "^6.0.1",
    "jwks-rsa": "^3.0.0",
    "next": "^13.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "babel-jest": "29.7.0",
    "cypress": "13.5.1",
    "eslint": "8.53.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "start-server-and-test": "2.0.3"
  }
}
